<div ng-app ng-controller="BeersController">

  <table>
    <thead>
    <th> <a>name</a> </th>
    <th> <a>style</a> </th>
    <th> <a>brewery</a> </th>
    </thead>
    <tr ng-repeat="beer in beers">
      <td>{{beer.name}}</td>
      <td>{{beer.style.name}}</td>
      <td>{{beer.brewery.name}}</td>
    </tr>
  </table>

</div>
